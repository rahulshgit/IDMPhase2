      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="FinancialImpact" type="cct:TextType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>A free text field for a description of the financial impact of the change request</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="BrandImpact" type="cct:TextType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>A free text field for a description of the brand impact of the change request</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TradingPeriodImpact" type="cct:TextType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>A free text field for a description of the critical trading period impact of the change request</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SiteQuantity" type="cct:QuantityType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The number of sites affected by the change request.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SitePercent" type="cct:PercentType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The percentage of sites affected by the change request.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Site" type="cct:TextType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>A free text field for the sites affected by the change request.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="UserQuantity" type="cct:QuantityType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The number of users affected by the change request.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="UserPercent" type="cct:PercentType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The percentage of users affected by the change request.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="User" type="cct:TextType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>A free text field for the users affected by the change request.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Extension" type="extvbo:ExtendedChangeRequestImpactType" minOccurs="0"/>
      </xsd:sequence>
