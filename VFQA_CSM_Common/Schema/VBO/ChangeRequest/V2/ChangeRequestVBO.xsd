   <xsd:complexType name="ChangeRequestServiceLevelType">
      <xsd:annotation>
         <xsd:documentation>A component taht holds details of the service level (SLA) being applied to the change request. This forms the basis of the expected timings and the escalation.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="cmn:BaseComponentType">
            <xsd:sequence>
               <xsd:element name="RequestedStartDateTime" type="cct:DateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>The date and time the service level starts for the change request.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="RequestedResolutionDateTime" type="cct:DateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>The date and time the requester has asked for the change request to be resolved by</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ActualResponseDateTime" type="cct:DateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>The date and time the change request was actually responded to</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="PromisedResponseDateTime" type="cct:DateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>The date and time the organisation has promised to the requester that the change request will be responded to</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="PromisedResolutionDateTime" type="cct:DateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>The date and time the organisation has promised/target to the requester that the change request be resolved by</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ActualResolutionDateTime" type="cct:DateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>The date and time that the organisation actually resolved the change request</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ScheduledStartDateTime" type="cct:DateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>The date and time the organisation estimates/schedules that the change request will start.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ScheduledCloseDateTime" type="cct:DateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>The date and time the organisation estimates/schedules that the change request will be closed after resolution</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EstimatedOnholdDateTime" type="cct:DateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>The date and time at which the incident should be transitioned out of an On-Hold state as proposed by the service level.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Extension" type="extvbo:ExtendedChangeRequestServiceLevelType" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   
