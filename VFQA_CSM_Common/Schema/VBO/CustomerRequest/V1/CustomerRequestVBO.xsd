   <xsd:annotation>
      <xsd:documentation>Schema definition for CustomerRequest.</xsd:documentation>
   </xsd:annotation>
   <xsd:complexType name="CustomerRequestVBOType">
      <xsd:annotation>
         <xsd:documentation>A Customer Request is a type of Business
				Interaction where the Customer is making a Request that Vodafone
				does something. A Customer Request will usually be completed by some
				sort of activity and a Response from Vodafone. The scope of Customer
				Request includes all frequent, lower risk, no cost request made by a
				customer, his contact or user of a product that are received by
				Vodafone over any channel.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="cmn:ExtendedBaseComponentType">
            <xsd:sequence>
               <xsd:element name="Details" type="tns:CustomerRequestDetailsType" minOccurs="0"/>
               <xsd:element name="RelatedCustomerRequests" minOccurs="0">
                  <xsd:complexType>
                     <xsd:sequence>
                        <xsd:element name="RelatedCustomerRequest" type="tns:RelatedCustomerRequestType" maxOccurs="unbounded"/>
                     </xsd:sequence>
                  </xsd:complexType>
               </xsd:element>
               <xsd:element name="Roles" type="tns:CustomerRequestRolesType" minOccurs="0"/>
               <xsd:element name="Parts" type="tns:CustomerRequestPartsType" minOccurs="0"/>
               <xsd:element name="Extension" type="extvbo:ExtendedCustomerRequestVBOType" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CustomerRequestDetailsType">
      <xsd:annotation>
         <xsd:documentation>A type definition for CustomerRequest Details</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="tns:CustomerRequestCommonDetailsType">
            <xsd:sequence>
               <xsd:element name="CustomerResolutionAcceptanceIndicator" type="cct:IndicatorType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Indicates the if the Customer is happy with
								the resolution of the Customer Request</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Extension" type="extvbo:ExtendedCustomerRequestCommonDetailsType" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CustomerRequestCommonDetailsType">
      <xsd:sequence>
         <xsd:element name="PriorityCode" type="cct:CodeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The priority that should be given to the
						Customer Request from Vodafone perspective</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SeverityCode" type="cct:CodeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The Severity that should be given to the
						Customer Request from Vodafone perspective</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="UrgencyCode" type="cct:CodeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The Urgency that should be given to the Customer
						Request from customer's perspective</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InstantResolutionIndicator" type="cct:IndicatorType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Indicates if the Service Request was resolved in
						the first call from the Customer.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AlertRequestorIndicator" type="cct:IndicatorType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>If the user would like to get alerted when there
						is any update on the Service Request, this flag should be checked</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RootCauseCode" type="cct:CodeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Root Cause Analysis Code</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RequestedResolveDateTime" type="cct:DateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Date and time customer expects resolution by.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RequestedRespondDateTime" type="cct:DateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Date and time customer expects response by</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ActualResponseDateTime" type="cct:DateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Date and time of response</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ActualResolutionDateTime" type="cct:DateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The date and Time on which the service was
						brought back to its original condition.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CloseRequestDateTime" type="cct:DateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Date on which the service request was closed.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ExpectedCloseDateTime" type="cct:DateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Target Close Date Time</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PromisedResponseDate" type="cct:DateType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Response Date agreed upon on the contract
						document.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ExternalReferenceID" type="cct:IDType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Customer external reference number.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SubReasonCode" type="cct:CodeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Customer Friendly Status based on Main Status
						and Sub reason of the Customer request</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="RelatedCustomerRequestType">
      <xsd:annotation>
         <xsd:documentation>A type definition for Related CustomerRequest Type</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="cmn:BaseComponentType">
            <xsd:sequence>
               <xsd:element name="Extension" type="extvbo:ExtendedRelatedCustomerRequestType" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CustomerRequestPartsType">
      <xsd:sequence>
         <xsd:element name="Attachments" minOccurs="0">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Attachment" type="tns:AttachmentsType" maxOccurs="unbounded"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Channels" minOccurs="0">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Channel" type="tns:ChannelsType" maxOccurs="unbounded"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="CustomerAccount" type="tns:CustomerAccountType" minOccurs="0"/>
         <xsd:element name="CustomerAgreement" type="tns:CustomerAgreementsType" minOccurs="0"/>
         <xsd:element name="Notes" minOccurs="0">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Note" type="tns:NotesType" maxOccurs="unbounded"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Subscriptions" minOccurs="0">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Subscription" type="tns:SubscriptionType" maxOccurs="unbounded"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Services" minOccurs="0">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Service" type="tns:ServiceType" maxOccurs="unbounded"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Resources" minOccurs="0">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Resource" type="tns:ResourceType" maxOccurs="unbounded"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="WorkLog" minOccurs="0">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="WorkInfo" type="tns:WorkLogInfoType" maxOccurs="unbounded"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Locations" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The set of locations associated with Customer
						Request.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Location" type="tns:LocationType" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation>The location which is associated to Customer
									Request.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Specification" type="tns:CustomerRequestSpecificationType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>A specification of the characteristics
						associated with a Customer Request.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EscalationPlan" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>When a Incident is escalated to someone, this
						information is captured in this element.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Escalation" type="tns:EscalationType" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation>Capture the escalation information for the
									customer request.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="EscalationHistory" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>This component will capture the history of escalations that have been made for the Customer Request.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Escalation" type="tns:EscalationHistoryType" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation>When an change request breaks one of its service levels then it is escalated to an appropriate party role.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Extension" type="extvbo:ExtendedCustomerRequestPartsType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>
