      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CustomerRequestSpecificationType">
      <xsd:annotation>
         <xsd:documentation>A type definition for Customer Request
				Specification Type</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="cmn:SpecificationType">
            <xsd:sequence>
               <xsd:element name="Extension" type="extvbo:ExtendedCustomerRequestSpecificationType" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="EscalationType">
      <xsd:annotation>
         <xsd:documentation>An escalation of the incident. Each records the
				date and time and the person who is being escalated to.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ID" type="cct:IDType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>An identifier or name for the escalation
						transition.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="NameText" type="cct:TextType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>A name assigned to the escalation of the
						incident.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DescText" type="cct:TextType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>A description of the escalation transition.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Type" type="cct:CodeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>A classification of the business object or
						component. This identifies the sub-type of the instance of object
						or component.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Categories" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The set of classifications for the business
						object or component.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Category" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation>A classification applied to the business
									object or component. More than one classification scheme can be
									applied using the @listName attribute to identify the type of
									code list being used. The classification scheme can be used to
									group instances of the object or component.</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:simpleContent>
                           <xsd:extension base="cct:CodeType">
                              <xsd:attribute name="listHierarchyID" type="xsd:string" use="optional">
                                 <xsd:annotation>
                                    <xsd:documentation>An indication of the hierarchy level of
													the category when involved in a hierarchical
													classification, e.g. level 1 ->
													Category/@listHierarchyID=1, level 2 ->
													Category/@listHierarchyID=2. In this case the listName
													should be the same for each level to show their
													relationship.</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:attribute>
                           </xsd:extension>
                        </xsd:simpleContent>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Status" type="cct:CodeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The lifecycle state of the business object or
						component. This field is used to track the specific state an
						instance of an object or component is in, e.g. the Customer Bill
						is Active or the Customer Request has been Cancelled.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EscalationIndicator" type="cct:IndicatorType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Indicates if the Incident was escalated or not.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EscalatedToPerson" type="tns:CustomerRequestSupportGroupType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The target escalation group/person ID of the
						transition.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EscalatedToGroup" type="tns:CustomerRequestSupportGroupType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The organisation that the incident has been
						escalated to.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EscalatedTypeCode" type="cct:CodeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The type of escalation being performed in
						response to the service level.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EffectiveDateTime" type="cct:DateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The date and time of the transition, i.e. the
						timestamp of when the target group/person ID was entered.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ReasonCode" type="cct:CodeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The code describing why the transition occured.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Level" type="cct:TextType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>A description to show the various escalation
						levels</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ContactPoint" type="cmn:ContactPointType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Anonymous Party reference</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Extension" type="extvbo:ExtendedEscalationType" minOccurs="0"/>
      </xsd:sequence>
      <xsd:attribute name="actionCode" type="cmn:ActionCodeType" use="optional">
         <xsd:annotation>
            <xsd:documentation>The @actionCode attribute is used within a
					modification operation to indicate the action (add, update, delete)
					intended on a component.</xsd:documentation>
         </xsd:annotation>
      </xsd:attribute>
   </xsd:complexType>
   <xsd:complexType name="SubscriberType">
      <xsd:complexContent>
         <xsd:extension base="cmn:BaseComponentType">
            <xsd:sequence>
               <xsd:element name="FirstName" type="cct:TextType" minOccurs="0"/>
               <xsd:element name="LastName" type="cct:TextType" minOccurs="0"/>
               <xsd:element name="ContactPoints" minOccurs="0">
                  <xsd:complexType>
                     <xsd:sequence>
                        <xsd:element name="ContactPoint" type="cmn:ContactPointType" minOccurs="0" maxOccurs="unbounded"/>
                     </xsd:sequence>
                  </xsd:complexType>
               </xsd:element>
               <xsd:element name="CostCentre" type="cct:TextType" minOccurs="0"/>
               <xsd:element name="BusinessUnit" type="cct:TextType" minOccurs="0"/>
               <xsd:element name="Extension" type="extvbo:ExtendedSubscriberType" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="WorkInfoAttachmentType">
      <xsd:complexContent>
         <xsd:extension base="cmn:AttachmentType">
            <xsd:sequence>
               <xsd:element name="Extension" type="extvbo:ExtendedAttachmentsType" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
