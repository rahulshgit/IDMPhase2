         <xsd:element name="OrganisationNameText" type="cct:TextType" minOccurs="0"/>
         <xsd:element name="ParentOrganisationID" type="cct:IDType" minOccurs="0"/>
         <xsd:element name="ParentOrganisationNameText" type="cct:TextType" minOccurs="0"/>
         <xsd:element name="Extension" type="extvbo:ExtendedAssignedToGroupType" minOccurs="0"/>
      </xsd:sequence>
      <xsd:attribute name="actionCode" type="cmn:ActionCodeType" use="optional"/>
   </xsd:complexType>
   <xsd:complexType name="ContactNameType">
      <xsd:complexContent>
         <xsd:extension base="cmn:BaseComponentType">
            <xsd:sequence>
               <xsd:element name="MiddleName" type="cct:TextType" minOccurs="0"/>
               <xsd:element name="FamilyName" type="cct:TextType" minOccurs="0"/>
               <xsd:element name="ContactPoints" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>The set of contact points for the requester
								role. These may be e-mail, postal address, telephone number,
								etc.</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                     <xsd:sequence>
                        <xsd:element name="ContactPoint" type="cmn:ContactPointType" minOccurs="1" maxOccurs="unbounded">
                           <xsd:annotation>
                              <xsd:documentation>A specific contact point for the requester.
											These may be e-mail, postal address, telephone number, etc.
											And may be differentiated as business, home, etc.</xsd:documentation>
                           </xsd:annotation>
                        </xsd:element>
                     </xsd:sequence>
                  </xsd:complexType>
               </xsd:element>
               <xsd:element name="Extension" type="extvbo:ExtendedContactNameType" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="LocationType">
      <xsd:complexContent>
         <xsd:extension base="cmn:BaseComponentType">
            <xsd:sequence>
               <xsd:element name="AccessInfoText" type="cct:TextType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>A free text description of the access
								information required to obtain access to the Location.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ContactPoints" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>The set of contact points for the location</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                     <xsd:sequence>
                        <xsd:element name="ContactPoint" type="cmn:ContactPointType" maxOccurs="unbounded">
                           <xsd:annotation>
                              <xsd:documentation>A specific contact point for the location.</xsd:documentation>
                           </xsd:annotation>
                        </xsd:element>
                     </xsd:sequence>
                  </xsd:complexType>
               </xsd:element>
               <xsd:element name="ContactName" type="tns:ContactNameType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>The name of the contact for the Location.</xsd:documentation>
                  </xsd:annotation>
